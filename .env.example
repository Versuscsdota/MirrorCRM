# MirrorCRM server configuration
PORT=8080
# Path to SQLite file for KV
KV_SQLITE_PATH=./data.sqlite
# Directory for file storage
FILES_DIR=./files
# Upload limits
MAX_UPLOAD_MB=50
MAX_FILES_PER_UPLOAD=10
# Files retention (days) for cleanup script
FILES_RETENTION_DAYS=90
# Cookie / session settings
SESSION_COOKIE_NAME=mirrorsid
SESSION_TTL_SECONDS=604800
# IMPORTANT: set a strong secret. Example: openssl rand -hex 32
SESSION_HMAC_SECRET=
